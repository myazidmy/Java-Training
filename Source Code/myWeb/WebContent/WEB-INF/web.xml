<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>HelloWorldServlet</servlet-name>
    <servlet-class>com.yazid.faiz.HelloServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>MyServlet1</servlet-name>
    <servlet-class>com.yazid.cookies.MyServlet1</servlet-class>
  </servlet>
  
   <servlet>
    <servlet-name>MyServlet2</servlet-name>
    <servlet-class>com.yazid.cookies.MyServlet2</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>EchoServlet</servlet-name>
    <servlet-class>com.yazid.faiz.EchoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QueryServlet</servlet-name>
    <servlet-class>com.yazid.faiz.QueryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWorldServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EchoServlet</servlet-name>
    <url-pattern>/Echo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryServlet</servlet-name>
    <url-pattern>/query</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>MyServlet1</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>MyServlet2</servlet-name>
    <url-pattern>/login1</url-pattern>
  </servlet-mapping>
  
</web-app>